<definition>
	<command name="viewDefaultSharableBookmarks">
		<for list="Facebook,LinkedIn,Twitter" param="socialBookmark">
			<execute function="AssertElementPresent" locator1="SocialBookmarks#SOCIAL_BOOKMARK_ICON">
				<var method="StringUtiL#toLowerCase('${socialBookmark}')" name="${key_socialBookmark}" />
			</execute>
		</for>

		<execute function="Click" locator1="Icon#SHARE" />

		<for list="Google+,AddThis,Delicious,Digg,Evernote,Newsvine,Reddit,Slashdot,StumbleUpon,Tuenti" param="socialBookmark">
			<execute function="AssertElementPresent" locator1="SocialBookmarks#DROPDOWN_ELEMENT">
				<var name="key_socialBookmark" value="${socialBookmark}" />
			</execute>
		</for>

		<execute function="Click" locator1="Icon#SHARE" />
	</command>
</definition>