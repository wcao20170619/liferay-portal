        "folder": {
          "folder": {
            "field": "folderId",
            "label": "folder",
            "parameter_name": "folder"
          }
        },
        "file-format": {
          "term_map": {
            "field": "extension",
            "multi_value": true,
            "multi_value_operator": "and",
            "size": 50,
            "name": "file-format",
            "filter_mode": "pre",
            "label": "file-format",
            "parameter_name": "extension",
            "frequency_threshold": "1",
            "parameters": {
              "map": [
                {
                  "values": [
                    "png",
                    "gif",
                    "jpeg",
                    "jpg",
                    "tiff"
