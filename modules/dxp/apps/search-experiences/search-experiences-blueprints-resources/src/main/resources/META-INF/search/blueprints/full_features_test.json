        "custom": [],
        "page": {
          "parameter_name": "page"
        },
        "sort": {
          "_geo_distance": {
            "unit": "km",
            "field": "expando__keyword__custom_fields__location_geolocation",
            "distance_type": "arc",
            "locations": [
              [
                "${ipstack.latitude}",
                "${ipstack.longitude}"
              ]
            ],
            "label": "Geodistance",
            "parameter_name": "sort4",
            "type": "geo_distance",
            "order": "asc"
          },
          "modified_sortable": {
            "label": "modified",
            "parameter_name": "sort3"
          },
          "_score": {
            "label": "score",
            "parameter_name": "sort1"
          },
          "localized_title${context.language_id}_sortable": {
            "label": "title",
            "parameter_name": "sort2"
          }
        }
      },
      "sort_configuration": [
        {
          "_score": {
            "order": "desc"
          }
        },
        {
          "modified_sortable": "desc"
        }
      ],
      "aggregation_configuration": {
        "cardinality-test": {
          "cardinality": {
            "precision_threshold": 3000,
            "field": "assetTagNames.raw",
            "description": "Number of unique asset tag names"
