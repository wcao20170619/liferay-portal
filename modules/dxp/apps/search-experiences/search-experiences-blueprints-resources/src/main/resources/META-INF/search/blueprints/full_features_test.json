              "ranges": [
                {
                  "from": "${time.current_date|modifier=-1h,dateFormat=yyyyMMddhhmmss}",
                  "label": "last-hour",
                  "to": ""
                },
                {
                  "from": "${time.current_date|modifier=-1d,dateFormat=yyyyMMddhhmmss}",
                  "label": "last-day",
                  "to": ""
                },
                {
                  "from": "${time.current_date|modifier=-1W,dateFormat=yyyyMMddhhmmss}",
                  "label": "last-week",
                  "to": ""
                },
                {
                  "from": "${time.current_date|modifier=-1M,dateFormat=yyyyMMddhhmmss}",
                  "label": "last-month",
                  "to": ""
                },
                {
                  "from": "${time.current_date|modifier=-1y,dateFormat=yyyyMMddhhmmss}",
                  "label": "last-year",
                  "to": ""
