{
	"general": {
		"searchableAssetTypes": [
			"com.liferay.wiki.model.WikiPage",
			"com.liferay.journal.model.JournalArticle"
		]
	},
	"queryConfiguration": {
		"applyIndexerClauses": false,
		"queryEntries": [
			{
				"clauses": [
					{
						"context": "query",
						"occur": "filter",
						"query": {
							"bool": {
								"must_not": [
									{
										"term": {
											"folderId": {
												"value": "${configuration.value}"
											}
										}
									}
								]
							}
						}
					}
				],
				"condition": {
				},
				"enabled": true
			}
		]
	}
}