{
	"queryConfiguration": {
		"applyIndexerClauses": false,
		"queryEntries": [
			{
				"clauses": [
					{
						"context": "query",
						"occur": "must",
						"query": {
							"function_score": {
								"boost": 100,
								"gauss": {
									"expando__custom_fields__location_geolocation": {
										"decay": 0.3,
										"origin": {
											"lat": "${configuration.lat}",
											"lon": "${configuration.lon}"
										},
										"scale": "1000km"
									}
								}
							}
						}
					}
				],
				"condition": {
				},
				"enabled": true
			}
		]
	}
}