{
	"$id": "http://www.liferay.com/schema/search/7.3.0/search-configuration.schema.json",
	"$schema": "http://www.liferay.com/schema/search/search-configuration.schema.json",
	"definitions": {
		"clause_configuration": {
			"properties": {
				"clauses": {
					"properties": {
						"configuration": {
							"type": "object"
						},
						"context": {
							"enum": [
								"query",
								"filter",
								"rescore"
							],
							"type": "string"
						},
						"occur": {
							"enum": [
								"must",
								"should",
								"must_not"
							],
							"type": "string"
						},
						"query_type": {
							"enum": [
								"boosting",
								"constant_score",
								"dismax",
								"exists",
								"function_score",
								"fuzzy",
								"geo_bounding_box",
								"geo_distance",
								"geo_geo_polygon",
								"geo_shape",
								"ids",
								"match",
								"match_all",
								"multi_match",
								"more_like_this",
								"nested",
								"percolate",
								"prefix",
								"range",
								"regexp",
								"script",
								"simple_string_query",
								"string_query",
								"term",
								"terms",
								"terms_set",
								"wildcard",
								"wrapper"
							],
							"type": "string"
						}
					},
					"required": [
						"context",
						"occur",
						"query_type",
						"configuration"
					],
					"type": "array"
				},
				"conditions": {
					"properties": {
						"configuration": {
							"type": "object"
						},
						"handler_name": {
							"type": "string"
						},
						"operator": {
							"enum": [
								"and",
								"or",
								"not"
							],
							"type": "string"
						}
					},
					"required": [
						"handler",
						"operator"
					],
					"type": "array"
				},
				"description": {
					"type": "array"
				},
				"enabled": {
					"type": "boolean"
				},
				"title": {
					"type": "array"
				}
			},
			"type": "object"
		},
		"misspelling": {
			"properties": {
				"keys": {
					"type": "array"
				},
				"value": {
					"type": "string"
				}
			},
			"type": "object"
		}
	},
	"description": "A representation of a search configuration.",
	"properties": {
		"clause_configuration": {
			"items": {
				"$ref": "#/definitions/clause_configuration"
			},
			"type": "array"
		},
		"misspellings": {
			"items": {
				"$ref": "#/definitions/misspelling"
			},
			"type": "array"
		},
		"synonyms": {
			"type": "array"
		}
	},
	"type": "object"
}