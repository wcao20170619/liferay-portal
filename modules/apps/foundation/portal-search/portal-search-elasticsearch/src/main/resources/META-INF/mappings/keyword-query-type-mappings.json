{
	"KeywordQueryDocumentType": {
		"dynamic_templates": [
			{
				"template_": {
					"mapping": {
						"index_options": "docs",
						"omit_norms": true,
						"type": "keyword"
					},
					"match": "*",
					"match_mapping_type": "string"
				}
			},
			{
				"template_keywordSearch": {
					"mapping": {
						"type": "text"
					},
					"match": "keywordSearch_*",
					"match_mapping_type": "string"
				}
			}
		],
		"properties": {
			"companyId": {
				"store": true,
				"type": "keyword"
			},
			"groupId": {
				"store": true,
				"type": "keyword"
			},
			"priority": {
				"store": true,
				"type": "double"
			},
			"uid": {
				"store": true,
				"type": "keyword"
			}
		}
	}
}